{
  "name": "Abur",
  "version": "1.0.0",
  "description": "",
  "exports": "./index.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node --loader esm-module-alias/loader --no-warnings index.js",
    "dev": "cross-env NODE_ENV=development nodemon --loader esm-module-alias/loader --no-warnings index.js",
    "test": "cross-env NODE_ENV=test node --loader esm-module-alias/loader --no-warnings --test",
    "db:migrate": "npx sequelize db:migrate",
    "db:seed": "npx sequelize db:seed:all",
    "db:win": "scripts\\refresh.bat",
    "db:linux": "./scripts/refresh.sh",
    "restore:win": "rmdir /s /q node_modules && pnpm i",
    "restore:linux": "rm -rf node_modules && pnpm i",
    "deploy": "fly deploy",
    "deploy:open": "fly apps open",
    "deploy:logs": "fly logs",
    "lint": "pnpx eslint .",
    "lint:fix": "pnpx eslint . --fix"
  },
  "aliases": {
    "@root": ".",
    "@core": "core",
    "@models": "models",
    "@graphql": "graphql",
    "@types": "graphql/types",
    "@modules": "core/module.js",
    "@services": "core/services",
    "@config": "config/config.js",
    "@repositories": "core/repositories",
    "@middleware": "core/middleware/index.js",
    "@auth-mutations": "graphql/mutations/auth/index.js",
    "@dlc-queries": "graphql/queries/dlc/index.js",
    "@dlc-mutations": "graphql/mutations/dlc/index.js",
    "@game-queries": "graphql/queries/game/index.js",
    "@game-mutations": "graphql/mutations/game/index.js",
    "@customer-queries": "graphql/queries/customer/index.js",
    "@customer-mutations": "graphql/mutations/customer/index.js",
    "@developer-queries": "graphql/queries/developer/index.js",
    "@developer-mutations": "graphql/mutations/developer/index.js",
    "@review-mutations": "graphql/mutations/review/index.js",
    "@review-queries": "graphql/queries/review/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "esm-module-alias": "^2.2.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "graphql": "^16.9.0",
    "graphql-date": "^1.0.3",
    "graphql-http": "^1.22.3",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@faker-js/faker": "^9.3.0",
    "@flydotio/dockerfile": "^0.5.9",
    "@stylistic/eslint-plugin-js": "^2.11.0",
    "eslint": "^9.16.0",
    "faker": "^6.6.6",
    "globals": "^15.13.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2"
  }
}
