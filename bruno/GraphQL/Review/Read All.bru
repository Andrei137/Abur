meta {
  name: Read All
  type: graphql
  seq: 2
}

post {
  url: {{url}}
  body: graphql
  auth: none
}

body:graphql {
  query ListReview {
    reviews {
      id
      rating
      comment
      game {
          ... on Game {
              id
              title
              genre
          }
          ... on DLC {
              id
              title
              baseGameId
          }
      }
    }
  }
}
