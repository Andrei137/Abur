meta {
  name: Create
  type: graphql
  seq: 4
}

post {
  url: {{url}}
  body: graphql
  auth: none
}

body:graphql {
  mutation CreateCustomer($customer: CustomerInput!) {
    createCustomer(customer: $customer) {
      id
      username
      password
      email
      
      firstName
      lastName
    }
  }
}

body:graphql:vars {
