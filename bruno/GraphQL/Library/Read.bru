meta {
  name: Read
  type: graphql
  seq: 1
}

post {
  url: {{url}}
  body: graphql
  auth: none
}

headers {
  Authorization: Bearer {{tokenCustomer}}
}

body:graphql {
  query GetLibrary {
    library {
      customer {
        firstName
      }
      games {
        id
        name
        price
        dlcs {
          name
          price
        }
      }
    }
  }
}
