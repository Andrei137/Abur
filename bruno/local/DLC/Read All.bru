meta {
  name: Read All
  type: graphql
  seq: 3
}

post {
  url: http://localhost:3001/graphql
  body: graphql
  auth: none
}

body:graphql {
  query ListDLCs {
    dlcs {
      id
      name
      releaseDate
      price
  
      developer {
        username
        email
  
        studio
        website
      }
      baseGame {
      	name
        price
      }
    }
  }
}
